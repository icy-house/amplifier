
# =============================================================================
# AMPLIFIER BACKEND ENVIRONMENT CONFIGURATION
# =============================================================================
# Copy this file to .env and fill in your actual values.
# Never commit the .env file to version control.

# Apple Sign-In Configuration
# Required for Apple Sign-In authentication
APPLE_CLIENT_ID=A39UD833JS.house.icy.amplemusic.service
# JWT token generated using your Apple Developer credentials
APPLE_CLIENT_SECRET=your_apple_client_secret_here

# Apple Music API Configuration
# Required for Apple Music API access
APPLE_MUSIC_KEY_ID=your_apple_music_key_id
# Your Apple Developer Team ID (10-character alphanumeric)
APPLE_MUSIC_TEAM_ID=A39UD833JS
# Issuer for Apple Music API (typically your Team ID)
APPLE_MUSIC_ISSUER=your_apple_music_issuer
# Path to your Apple Music API private key file (.p8)
APPLE_MUSIC_PRIVATE_KEY_PATH=path_to_your_private_key.p8

# Firebase Configuration
# Your Firebase project ID for the Amplifier backend
AMPLIFIER_PROJECT_ID=amplifier-backend
